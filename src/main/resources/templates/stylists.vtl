<!--stylists.vtl-->
<h1>All stylists</h1>

  #if ($stylists.size() != 0)
    <ul>
      #foreach ($stylist in $stylists)
        <li><a href="/stylists/$stylist.getId()">$stylist.getName()</a></li>
      #end
    </ul>
  #else
    <p>No stylists. Get some!</p>
  #end

<a href="/">Add New Stylist</a>
